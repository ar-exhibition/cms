@model cms.ar.xarchitecture.de.Models.Wrapper.AssetSubmissionValues

@{
    ViewData["Title"] = "Check your data";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Check your data!</h1>

<div>
    <h4>SceneAsset</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.creator)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.programme)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.course)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.assetName)
        </dd>
    </dl>
</div>

<div class="form-group">
    <input type="submit" value="Submit Model" class="btn btn-primary" />
</div>