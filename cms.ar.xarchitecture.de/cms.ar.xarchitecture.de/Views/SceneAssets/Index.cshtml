@model IEnumerable<cms.ar.xarchitecture.de.cmsXARCH.SceneAsset>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.AssetName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FileUuid)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ExternalLink)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CreationDate)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Deleted)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CourseNavigation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CreatorNavigation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ThumbnailUu)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.AssetName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FileUuid)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ExternalLink)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CreationDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Deleted)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CourseNavigation.CourseId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CreatorNavigation.CreatorId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ThumbnailUu.ThumbnailUuid)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.AssetId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.AssetId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.AssetId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
